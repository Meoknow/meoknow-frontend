.neko-box{
  }
  .neko-box image{
    display: block;
    margin-top:10px;
    margin-bottom:20px;
    margin-left: auto;
    margin-right: auto;
    width: 650rpx;
    height: 650rpx;
    border-radius: 10px;
    }
  .neko-box .neko-text{
    margin-left: 50rpx;
    text-align: left;
    font-size: 50rpx;
  }

  .text{ 
    margin-top:40rpx;
    margin-left:50rpx;
  }

  .text .list{
    display: inline-block;
    width: 315rpx;
    margin-right: 20rpx;
    margin-bottom: 20rpx;
    border-radius: 10px;
  }
  .text .list .title{
    color: rgb(0, 204, 255);
    margin-bottom:0rpx;
    font-size:30rpx;
  }
  .text .list .female{
    color: rgb(247, 4, 255);
  }
  
  .text .list .male{
    color: rgb(4, 29, 255);
  }
  .text .list .content{
    margin-bottom:10rpx;
    font-size:40rpx;
  }